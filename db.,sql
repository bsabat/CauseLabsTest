CREATE TABLE `customer2` (
	`id`	INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
	`nameFirst`	TEXT,
	`nameLast`	TEXT NOT NULL,
	`email`	TEXT NOT NULL,
	`address`	TEXT,
	`phone`	TEXT,
	`twitter`	TEXT,
	`facebook`	TEXT
);

CREATE INDEX 'name' ON customer(nameLast);
CREATE INDEX 'email' ON customer(email);


